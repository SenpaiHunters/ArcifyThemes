{
  "manifest_version": 3,
  "name": "Arc Background Tint",
  "version": "1.4.2",
  "description": "Extension to customize YT or somthin.",
  "author": "idk",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "notifications",
    "host_permissions"
  ],
  "action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["options.js", "content.js"]
    }
  ],
  "host_permissions": ["https://www.youtube.com/*"]
}
